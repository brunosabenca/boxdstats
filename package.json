{
  "name": "boxdstats-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "nodemon --no-stdin --exec bin/server",
    "client": "node ./bin/client"
  },
  "nodemonConfig": {
    "delay": "3000",
    "ignore": [
      "client/"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.3",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "express-redis-cache": "^1.1.3",
    "flatted": "^2.0.0",
    "morgan": "~1.9.0",
    "node-fetch": "^2.6.1",
    "redis": "^3.1.1",
    "require-directory": "^2.1.1",
    "url-search-params-polyfill": "^5.0.0",
    "uuid": "^3.3.2",
    "uuid-random": "^1.0.6"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.10"
  }
}
